from os import system 
system ("cls")
import random
# estos condicionales me permitieron que mesa se escoja aleatoriamente
puesto=input("seleccione su puesto de votacion")
if puesto == "INSTITUCION EDUCATIVA RAFAEL SALAZAR":
    mesa=random.randrange(1,21)
    print(f"tu mesa de votacion es{mesa}")
if puesto == "INSTITUCION EDUCATIVA DE PROMOCION SOCIAL":
    mesa= random.randrange(1,2)
    print(f"tu mesa de votacion es la{mesa}")